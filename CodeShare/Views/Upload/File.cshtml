@using Microsoft.AspNet.Identity

@{
    ViewBag.Title = "UploadFile";
}

<h2>Hello @User.Identity.GetUserName()UploadFile</h2>

@using (Html.BeginForm("File", "Upload", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        @Html.TextBox("file", "", new { type = "file", @class = "form-control" }) <br />
        <input type="submit" class="form-control" value="Upload" id="FileUploadInput" />
        @ViewBag.Message
    </div>
}